@article{Machado2012,
abstract = {A alta incid\^{e}ncia de radia\c{c}\~{a}o solar sobre a cobertura das instala\c{c}\~{o}es av\'{\i}colas \'{e} um dos principais fatores que causam estresse t\'{e}rmico nos animais. Desta maneira, objetivou-se, com o presente trabalho, dimensionar e avaliar um sistema de resfriamento da cobertura de galp\~{o}es comerciais para cria\c{c}\~{a}o de frangos de corte, com vista ao uso de \'{a}gua de chuva, no munic\'{\i}pio de Rio Verde (GO). Durante o dimensionamento do sistema, foi considerado o resfriamento da cobertura a partir da aspers\~{a}o de \'{a}gua armazenada durante o per\'{\i}odo chuvoso, sendo denominado de Sistema de Resfriamento de Cobertura Sustent\'{a}vel (SRCS). Para avalia\c{c}\~{a}o do sistema SRCS, foram utilizados seis galp\~{o}es comerciais, os quais foram climatizados com ventila\c{c}\~{a}o for\c{c}ada em modo t\'{u}nel (press\~{a}o negativa) e sistema de resfriamento por nebuliza\c{c}\~{a}o interna. Em tr\^{e}s destes galp\~{o}es foi implementado o resfriamento adicional da cobertura (SRCS) e nas tr\^{e}s instala\c{c}\~{o}es restantes foi utilizado apenas o sistema convencional de climatiza\c{c}\~{a}o (SCC) especificado anteriormente. Durante o per\'{\i}odo compreendido entre o 21º e o 42º dia de vidas das aves, foram quantificados tr\^{e}s \'{\i}ndices de conforto t\'{e}rmico (\'{\i}ndice de temperatura de globo negro e umidade, carga t\'{e}rmica de radia\c{c}\~{a}o e entalpia espec\'{\i}fica do ar) e a mortalidade dos animais. Os resultados indicaram que o resfriamento adicional da cobertura reduziu, significativamente, a mortalidade das aves na \'{u}ltima semana de vida, fato que foi considerado como uma das maiores vantagens do sistema. Com os resultados obtidos foi ajustado os par\^{a}metros de um modelo estat\'{\i}stico exponencial que permite estimar a mortalidade dos animais a partir do 21° dia de vida, para os sistemas SRCS e SCC, em fun\c{c}\~{a}o da idade das aves e entalpia espec\'{\i}fica do ar.},
author = {Machado, Neiton Silva and Tin\^{o}co, Ilda de F\'{a}tima Ferreira and Zolnier, S\'{e}rgio and Mogami, Cristina Akemi and Damasceno, Fl\'{a}vio Alves and Zeviani, Walmes Marques},
doi = {10.3738/nucleus.v9i2.718},
file = {:home/walmes/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Machado et al. - 2012 - Resfriamento da cobertura de avi\'{a}rios e seus efeitos na mortalidade e nos \'{\i}ndices de conforto t\'{e}rmico.pdf:pdf},
issn = {1982-2278},
journal = {Nucleus},
keywords = {bem estar animal,frangos de corte,sistema de resfriamento adiab\'{a}tico},
language = {pt},
month = jan,
number = {2},
title = {{Resfriamento da cobertura de avi\'{a}rios e seus efeitos na mortalidade e nos \'{\i}ndices de conforto t\'{e}rmico}},
url = {http://www.nucleus.feituverava.com.br/index.php/nucleus/article/view/718},
volume = {9},
year = {2012}
}
