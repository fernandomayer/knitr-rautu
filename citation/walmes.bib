issn = {1679-0359},
journal = {Semina: Ci\^{e}ncias Agr\'{a}rias},
keywords = {Gessagem,Gypsum application,No tillage and tillage,Perfil do solo.,Plantio direto e convencional,Soil profile.},
language = {en},
month = oct,
number = {5},
pages = {1717--1732},
title = {{Efeito do gesso nas propriedades qu\'{\i}micas do solo sob dois sistemas de manejo}},
url = {http://www.uel.br/revistas/uel/index.php/semagrarias/article/view/7814},
